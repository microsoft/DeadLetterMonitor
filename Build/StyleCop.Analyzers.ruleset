<?xml version="1.0" encoding="utf-8"?>
<RuleSet Name="Custom Rules for StyleCop.Analyzers" Description="Custom Rules for StyleCop.Analyzers" ToolsVersion="14.0">
  <Rules AnalyzerId="StyleCop.Analyzers" RuleNamespace="StyleCop.Analyzers">
    <!-- Two sibling elements which each start on their own line have different levels of indentation.-->
    <Rule Id="SA1137" Action="None" />
